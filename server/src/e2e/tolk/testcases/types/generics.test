========================================================================
Type instantiation with single argument
========================================================================
struct Generic<T> {}

fun main() {
    val a: Generic<int> {};
    //! ^ Generic<int>
}
------------------------------------------------------------------------
ok

========================================================================
Type instantiation with several arguments
========================================================================
struct Generic<T, U, W> {}

fun main() {
    val a: Generic<int, slice, bool> {};
    //! ^ Generic<int, slice, bool>
}
------------------------------------------------------------------------
ok

========================================================================
Type instantiation with other generic
========================================================================
struct Generic<T> {}

struct Container<T> {
    data: Generic<T>,
}

fun main() {
    val a: Container<int> {};
    val data = a.data;
    //! ^ Generic<int>
}
------------------------------------------------------------------------
ok
